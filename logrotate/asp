# Keep three weeks worth of temperature logs
/data/temp.txt {
        daily
        rotate 21
        compress
        delaycompress
        copytruncate
        ifempty
}
# Keep three weeks worth of board and FEE power (current draw) logs
/data/*-power.txt {
        daily
        rotate 21
        compress
        delaycompress
        copytruncate
        ifempty
}

# Keep three weeks worth of asp_cmnd logs
/lwa/runtime/runtime.log {
        daily
        rotate 21
        compress
        delaycompress
        copytruncate
        ifempty
}
